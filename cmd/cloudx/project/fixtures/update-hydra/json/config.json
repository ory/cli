{
  "clients": {
    "http": {
      "disallow_private_ip_ranges": true
    }
  },
  "dev": true,
  "hsm": {
    "enabled": false
  },
  "oauth2": {
    "client_credentials": {
      "default_grant_allowed_scope": false
    },
    "expose_internal_errors": true,
    "grant": {
      "jwt": {
        "iat_optional": false,
        "jti_optional": false,
        "max_ttl": "720h0m0s"
      }
    },
    "hashers": {
      "algorithm": "pbkdf2",
      "pbkdf2": {
        "iterations": 10000
      }
    },
    "pkce": {
      "enforced": true,
      "enforced_for_public_clients": true
    },
    "session": {
      "allowed_top_level_claims": [],
      "encrypt_at_rest": true,
      "exclude_not_before_claim": false
    }
  },
  "oidc": {
    "dynamic_client_registration": {
      "enabled": true
    },
    "subject_identifiers": {}
  },
  "serve": {
    "admin": {
      "cors": {
        "allow_credentials": true,
        "allowed_headers": [
          "Accept",
          "Content-Type",
          "Content-Length",
          "Accept-Language",
          "Content-Language",
          "Authorization"
        ],
        "allowed_methods": [
          "POST",
          "GET",
          "PUT",
          "PATCH",
          "DELETE",
          "CONNECT",
          "HEAD",
          "OPTIONS",
          "TRACE"
        ],
        "debug": false,
        "enabled": false,
        "exposed_headers": [
          "Cache-Control",
          "Expires",
          "Last-Modified",
          "Pragma",
          "Content-Length",
          "Content-Language",
          "Content-Type"
        ],
        "max_age": 0
      },
      "tls": {
        "enabled": false
      }
    },
    "cookies": {
      "same_site_legacy_workaround": true,
      "same_site_mode": "Lax",
      "secure": false
    },
    "public": {
      "cors": {
        "allow_credentials": true,
        "allowed_headers": [
          "Accept",
          "Content-Type",
          "Content-Length",
          "Accept-Language",
          "Content-Language",
          "Authorization"
        ],
        "allowed_methods": [
          "POST",
          "GET",
          "PUT",
          "PATCH",
          "DELETE",
          "CONNECT",
          "HEAD",
          "OPTIONS",
          "TRACE"
        ],
        "debug": false,
        "enabled": false,
        "exposed_headers": [
          "Cache-Control",
          "Expires",
          "Last-Modified",
          "Pragma",
          "Content-Length",
          "Content-Language",
          "Content-Type"
        ],
        "max_age": 0
      },
      "tls": {
        "enabled": false
      }
    },
    "tls": {
      "enabled": false
    }
  },
  "strategies": {
    "access_token": "jwt",
    "scope": "exact"
  },
  "ttl": {
    "access_token": "2h0m0s",
    "auth_code": "30m1s",
    "id_token": "1h0m1s",
    "login_consent_request": "30m1s",
    "refresh_token": "720h0m1s"
  },
  "urls": {
    "consent": "/custom-consent",
    "error": "/custom-error",
    "login": "/custom-login",
    "post_logout_redirect": "/custom-logout"
  },
  "webfinger": {
    "jwks": {},
    "oidc_discovery": {}
  }
}
